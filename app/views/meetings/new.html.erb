<h1> Calendar </h1>

<%= form_with model: @meeting, url: meeting_path do |form| %>

  <%= form.hidden_field :user_id, id: :meeting_user_id, value: current_user.id %>

  <div class="mb-3">
    <%= form.label :event, "Event" %>
    <%= form.text_field :event %>
  </div>

  <div class="mb-3">
    <%= form.label :start_time %>
    <%= form.datetime_field :start_time, class: "form-control", placeholder: "apple@gmail.com" %>
  </div>

  <div class="mb-3">
    <%= form.label :end_time %>
    <%= form.datetime_field :end_time, class: "form-control", placeholder: "apple@gmail.com" %>
  </div>

  <div class="mb-3">
    <%= form.submit "Add Event"%>
  </div>

<% end %>